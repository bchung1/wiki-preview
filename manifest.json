{
	"manifest_version": 2,
	"name" : "wiki-preview",
	"version": "1.0",
	"description": "Preview links on hover",
	"homepage_url": "https://github.com/bchung1/wiki-preview",
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"content_scripts": [
	{
		"matches": ["https://en.wikipedia.org/wiki/*"],
		"js": ["js_plugins/jquery.min.js", "js_plugins/jquery.hoverIntent.js", "content.js"],
		"css": ["css_plugins/balloon.min.css"]
	}
	]
}
